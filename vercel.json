{
  "buildCommand": "npm run build",
  "installCommand": "npm install --production=false",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NODE_VERSION": "18.14.2"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 